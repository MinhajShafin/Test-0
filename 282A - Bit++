#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,i,j,value=0;
    char x[4];
    cin>>n;
    for(i=0;i<n;i++){
    cin>>x;
    for(j=0;j<3;j++){
        if(x[j]!=' '&&x[j]=='+'){
        value++;
        break; }
        else if(x[j]!=' '&&x[j]=='-'){
        value--;
        break; }}}
    cout<<value;
    return 0;}
